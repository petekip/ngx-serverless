{
  "name": "ngx-serverless-starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run ngx-serverless-starter:serve-ssr",
    "serve:ssr": "node dist/ngx-serverless-starter/server/main.js",
    "build:ssr": "ng build && ng run ngx-serverless-starter:server",
    "prerender": "ng run ngx-serverless-starter:prerender",
    "dev:sls": "ng build -c development && ng run ngx-serverless-starter:serverless -c development && npm run serve:sls",
    "build:sls": "ng build -c development && ng run ngx-serverless-starter:serverless -c development && npm run serve:sls",
    "serve:sls": "serverless offline start --noPrependStageInUrl",
    "deploy": "serverless deploy",
    "terminate": "serverless remove"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/platform-server": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@nguniversal/express-engine": "^13.0.2",
    "@vendia/serverless-express": "^4.5.3",
    "aws-serverless-express": "^3.4.0",
    "express": "^4.15.2",
    "rxjs": "~7.4.0",
    "serverless-apigw-binary": "^0.4.4",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.4",
    "@angular/cli": "~13.1.4",
    "@angular/compiler-cli": "~13.1.0",
    "@nguniversal/builders": "^13.0.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "serverless": "^2.72.2",
    "serverless-offline": "^8.3.1",
    "typescript": "~4.5.2"
  }
}
